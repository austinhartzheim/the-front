{
    "description": "Monitor requests to CloudFront domains",
    "manifest_version": 2,
    "name": "The Front",
    "version": "1.0",

    "permissions": [
        "webRequest",
        "storage",
        "downloads"
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": "icons/addon.svg"
    }
}
